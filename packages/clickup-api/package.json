{
  "name": "@clickup/api",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist",
    "update-openapi": "curl -o openapi/clickup-api-v2-reference.json https://developer.clickup.com/openapi/clickup-api-v2-reference.json",
    "generate": "orval --config ./orval.config.ts",
    "generate:watch": "orval --config ./orval.config.ts --watch"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "orval": "8.0.0-rc.2",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}